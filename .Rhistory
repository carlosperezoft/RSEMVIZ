political_democracy_model <- '
# measurement model
ind60 =~ a*x1 + b*x2 + c*x3
dem60 =~ d*y1 + e*y2 + f*y3 + g*y4
dem65 =~ h*y5 + i*y6 + j*y7 + k*y8
# structural model: Regressions
dem60 ~ alpha*ind60
dem65 ~ direct*ind60 + beta*dem60
# effects of interest: mediation
indirect := alpha * beta
total := indirect + direct
proportion := indirect/total
#
# Correlations
y1 ~~ y5
y2 ~~ y4 + y6
y3 ~~ y7
y4 ~~ y8
y6 ~~ y8
'
